[cells]
  root = .
  prelude = prelude
  toolchains = toolchains
  none = none
  buckal = buckal

[cell_aliases]
  config = prelude
  ovr_config = prelude
  fbcode = none
  fbsource = none
  fbcode_macros = none
  buck = none

[external_cells]
  prelude = bundled

[parser]
  target_platform_detector_spec = target:root//...->prelude//platforms:default

[project]
  ignore = .git .buckd .idea .vscode .zed .DS_Store buck-out build